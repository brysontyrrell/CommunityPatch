{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "pattern": "^[^@\\s]+@[^@\\s]+\\.[^@\\s]+$",
      "examples": [
        "bryson.tyrrell@gmail.com"
      ]
    },
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "pattern": "^\\S+$",
      "examples": [
        "brysontyrrell"
      ]
    }
  },
  "required": [
    "email",
    "name"
  ]
}
